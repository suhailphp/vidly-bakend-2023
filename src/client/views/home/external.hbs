<div class="d-flex flex-wrap flex-stack mb-6">
        <!--begin::Heading-->
        <h3 class="fw-bold my-2">{{__ 'External trainings'}}
        <span class="fs-6 text-gray-400 fw-semibold ms-1">{{courseCount}} {{__ 'active course'}}</span></h3>
        <!--end::Heading-->
        <!--begin::Actions-->
        <div class="d-flex flex-wrap my-2">
        
            {{!-- <a href="#" class="btn btn-primary btn-sm" data-bs-toggle="modal" data-bs-target="#kt_modal_create_project">New Project</a> --}}
        </div>
        <!--end::Actions-->
    </div>

    <!--begin::Content-->
        <div class="d-flex flex-row-fluid me-xl-9 mb-10 mb-xl-0 mt-5">
            <!--begin::Pos food-->
            <div class="card card-flush card-p-0 bg-transparent " style="width: 100%;">
                <div class="card-body">
                    <ul class="nav nav-pills d-flex justify-content-between nav-pills-custom gap-3 mb-6">

                        {{#each courseDepartment}}
                            <li class="nav-item mb-3 me-0">
                                <!--begin::Nav link-->
                                <a class="nav-link nav-link-border-solid btn btn-outline btn-flex btn-active-color-primary flex-column flex-stack pt-9 pb-7 page-bg {{#if this.active}}show active{{/if}}" 
                                    data-bs-toggle="pill" href="#kt_ext_course_{{this.courseDepartmentID}}" style="width: 138px;height: 180px">
                                    <!--begin::Icon-->
                                    <div class="nav-icon mb-3">
                                        {{#if this.courseDepartmentDocumentID}}
                                            <img src="{{CONFIG.BASE_URL}}courseDepartment/document/{{this.courseDepartmentID}}" class="w-50px" alt="" />
                                        {{else}}
                                            <img src="{{CONFIG.BASE_URL}}dist/assets/media/psa.png" class="w-50px" alt="" />
                                        {{/if}}
                                    </div>
                                    <!--end::Icon-->
                                    <!--begin::Info-->
                                    <div class="">
                                        <span class="text-gray-800 fw-bold fs-6 d-block">{{? ../LNG '==' 'ar' this.nameAr this.nameEn}}</span>
                                        <span class="text-gray-400 fw-semibold fs-7">{{this.courseCount}}</span>
                                    </div>
                                    <!--end::Info-->
                                </a>
                                <!--end::Nav link-->
                            </li>
                        {{/each}}
                    </ul>

                    <div class="tab-content">
                        {{#each courseDepartment}}
                            <div class="tab-pane fade {{#if this.active}}show active{{/if}}" id="kt_ext_course_{{this.courseDepartmentID}}">
                                <div class="row g-6 g-xl-9">
                                    {{#each this.course}}
                                        <div class="col-md-6 col-xl-4">
                                            <a style="cursor: pointer;" onclick="ViewCourse({{this.courseID}})" class="card border-hover-primary">
                                                <!--begin::Card header-->
                                                <div class="card-header border-0 pt-9">
                                                    <!--begin::Card Title-->
                                                    <div class="card-title m-0">
                                                        <!--begin::Avatar-->
                                                        <div class="symbol symbol-50px w-50px bg-light">
                                                            {{#if this.Course.courseDocumentID}}
                                                                <img src="{{../CONFIG.BASE_URL}}course/document/{{this.courseID}}" alt="image" class="p-3" />
                                                            {{else}}
                                                                <img src="{{../CONFIG.BASE_URL}}dist/assets/media/psa.png" alt="image" class="p-3" />
                                                            {{/if}}
                                                        </div>
                                                    </div>
                                                    <div class="card-toolbar">
                                                        {{#if this.isHeEnrolled}}
                                                            <span class="badge badge-light-success fw-bold me-auto px-4 py-3">{{__ 'Enrolled'}}</span>
                                                        {{else}}
                                                            <span class="badge badge-light-warning fw-bold me-auto px-4 py-3">{{__ 'Not enrolled'}}</span>
                                                        {{/if}}
                                                        
                                                    </div>
                                                </div>
                                                <div class="card-body p-9">
                                                    <!--begin::Name-->
                                                    <div class="fs-4 fw-bold text-dark">{{? ../../LNG '==' 'ar' this.Course.nameAr this.Course.nameEn}}</div>
                                                    <p class="text-gray-400 fw-semibold fs-5 mt-1 mb-7">{{this.Course.location}}</p>
                                                    <div class="d-flex flex-wrap mb-5">
                                                        <!--begin::Due-->
                                                        <div class="border border-gray-300 border-dashed rounded min-w-125px py-3 px-4 me-7 mb-3">
                                                            <div class="fs-6 text-gray-800 fw-bold">{{simpleDate this.Course.startDate}}</div>
                                                            <div class="fw-semibold text-gray-400">{{__ 'Start date'}}</div>
                                                        </div>
                                                        <!--end::Due-->
                                                        <!--begin::Budget-->
                                                        <div class="border border-gray-300 border-dashed rounded min-w-125px py-3 px-4 mb-3">
                                                            <div class="fs-6 text-gray-800 fw-bold">{{simpleDate this.Course.endDate}}</div>
                                                            <div class="fw-semibold text-gray-400">{{__ 'End Date'}}</div>
                                                        </div>
                                                        <!--end::Budget-->
                                                    </div>

                                                    {{#if this.Course.employee}}
                                                        <div class="symbol-group symbol-hover mb-3">
                                                            {{#each this.Course.employee}}
                                                                {{#compare @index '<=' 10}}
                                                                    <div class="symbol symbol-label symbol-35px symbol-circle" data-bs-toggle="tooltip" title="{{? ../../LNG '==' 'ar' this.fullNameAr this.fullNameEn}}">
                                                                        {{#if this.profilePhotoDocumentID}}
                                                                            <img src="{{CONFIG.BASE_URL}}employee/document/{{this.employeeID}}" class="w-100" style="cursor:pointer"/>
                                                                        {{else}}
                                                                            {{{nameToIcon this.fullNameAr this.fullNameEn ../.../LNG}}}
                                                                        {{/if}}
                                                                    </div>
                                                                {{/compare}}
                                                            {{/each}}
                                                            
                                                        </div>
                                                    {{/if}}
                                    
                                                    {{!-- <div class="symbol-group symbol-hover">
                                                        
                                                        <!--begin::User-->
                                                        <div class="symbol symbol-35px symbol-circle" data-bs-toggle="tooltip" title="Emma Smith">
                                                            <img alt="Pic" src="{{CONFIG.BASE_URL}}/dist/assets/media/avatars/300-6.jpg" />
                                                        </div>
                                                        <!--begin::User-->
                                                        <!--begin::User-->
                                                        <div class="symbol symbol-35px symbol-circle" data-bs-toggle="tooltip" title="Rudy Stone">
                                                            <img alt="Pic" src="{{CONFIG.BASE_URL}}/dist/assets/media/avatars/300-1.jpg" />
                                                        </div>
                                                        <!--begin::User-->
                                                        <!--begin::User-->
                                                        <div class="symbol symbol-35px symbol-circle" data-bs-toggle="tooltip" title="Susan Redwood">
                                                            <span class="symbol-label bg-primary text-inverse-primary fw-bold">S</span>
                                                        </div>
                                                        <!--begin::User-->
                                                    </div> --}}
                                                    <!--end::Users-->
                                                </div>
                                                <!--end:: Card body-->
                                            </a>
                                            <!--end::Card-->
                                        </div>
                                    {{/each}}
                                </div>
                            </div>
                        {{/each}}

                
                       
                    </div>
                    <!--end::Tab Content-->
                </div>
                <!--end: Card Body-->
            </div>
            <!--end::Pos food-->
        </div>
        <!--end::Content-->

   